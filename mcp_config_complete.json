{
  "mcpServers": {
    "dubai-rest-api": {
      "command": "node",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/dubai-rest/index.js"],
      "env": {
        "DUBAI_REST_API_KEY": "${DUBAI_REST_API_KEY}"
      }
    },
    
    "dld-open-data": {
      "command": "node",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/dld-data/index.js"],
      "env": {
        "DLD_API_ENDPOINT": "https://www.dubailand.gov.ae/en/open-data"
      }
    },
    
    "property-finder": {
      "command": "node",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/property-finder/index.js"],
      "env": {
        "PROPERTY_FINDER_API_KEY": "${PROPERTY_FINDER_API_KEY}"
      }
    },
    
    "bayut-listings": {
      "command": "node",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/bayut-listings/index.js"],
      "env": {
        "BAYUT_API_KEY": "${BAYUT_API_KEY}"
      }
    },
    
    "dubizzle-scraper": {
      "command": "python",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/dubizzle/server.py"],
      "env": {
        "DUBIZZLE_CACHE_DIR": "/tmp/dubizzle-cache"
      }
    },
    
    "property-monitor": {
      "command": "node",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/property-monitor/index.js"],
      "env": {
        "PROPERTY_MONITOR_API_KEY": "${PROPERTY_MONITOR_API_KEY}"
      }
    },
    
    "propertyfinder-analytics": {
      "command": "node",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/pf-analytics/index.js"],
      "env": {
        "PF_ANALYTICS_KEY": "${PROPERTY_FINDER_API_KEY}"
      }
    },
    
    "chiller-rates-scraper": {
      "command": "python",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/chiller-scraper/server.py"],
      "env": {
        "SCRAPER_CACHE_DIR": "/tmp/chiller-cache"
      }
    },
    
    "social-intelligence": {
      "command": "python",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/social-listener/server.py"],
      "env": {
        "REDDIT_CLIENT_ID": "${REDDIT_CLIENT_ID}",
        "REDDIT_CLIENT_SECRET": "${REDDIT_CLIENT_SECRET}",
        "FACEBOOK_ACCESS_TOKEN": "${FACEBOOK_ACCESS_TOKEN}"
      }
    },
    
    "economic-data": {
      "command": "node",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/economic-data/index.js"],
      "env": {
        "FRED_API_KEY": "${FRED_API_KEY}",
        "TRADING_ECONOMICS_KEY": "${TRADING_ECONOMICS_KEY}"
      }
    },
    
    "mollak-integration": {
      "command": "python",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/mollak/server.py"],
      "env": {
        "MOLLAK_USERNAME": "${MOLLAK_USERNAME}",
        "MOLLAK_PASSWORD": "${MOLLAK_PASSWORD}"
      }
    },
    
    "reidin-data": {
      "command": "node",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/reidin/index.js"],
      "env": {
        "REIDIN_API_KEY": "${REIDIN_API_KEY}"
      }
    },
    
    "mortgage-calculator": {
      "command": "python",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/mortgage-calc/server.py"],
      "env": {
        "UAE_BANKS_DATA": "./data/banks.json"
      }
    },
    
    "price-predictor": {
      "command": "python",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/price-predictor/server.py"],
      "env": {
        "MODEL_PATH": "./models/price_prediction_model.pkl"
      }
    },
    
    "roi-calculator": {
      "command": "python",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/roi-calculator/server.py"]
    },
    
    "competitor-monitor": {
      "command": "python",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/competitor-monitor/server.py"],
      "env": {
        "COMPETITORS": "propertyfinder,bayut,dubizzle"
      }
    },
    
    "telegram-bot-backend": {
      "command": "python",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/telegram-bot/server.py"],
      "env": {
        "TELEGRAM_BOT_TOKEN": "${TELEGRAM_BOT_TOKEN}",
        "TELEGRAM_ADMIN_IDS": "${TELEGRAM_ADMIN_IDS}"
      }
    },
    
    "user-analytics": {
      "command": "python",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/user-analytics/server.py"],
      "env": {
        "ANALYTICS_DB": "${DATABASE_URL}"
      }
    },
    
    "subscription-manager": {
      "command": "node",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/subscription/index.js"],
      "env": {
        "STRIPE_SECRET_KEY": "${STRIPE_SECRET_KEY}",
        "DATABASE_URL": "${DATABASE_URL}"
      }
    },
    
    "lead-generator": {
      "command": "python",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/lead-gen/server.py"],
      "env": {
        "CRM_INTEGRATION": "${CRM_API_KEY}"
      }
    },
    
    "whatsapp-integration": {
      "command": "node",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/whatsapp/index.js"],
      "env": {
        "WHATSAPP_API_KEY": "${WHATSAPP_API_KEY}",
        "TWILIO_ACCOUNT_SID": "${TWILIO_ACCOUNT_SID}",
        "TWILIO_AUTH_TOKEN": "${TWILIO_AUTH_TOKEN}"
      }
    },
    
    "email-reports": {
      "command": "node",
      "args": ["/path/to/dubai-estate-agent/mcp-servers/email-reports/index.js"],
      "env": {
        "SENDGRID_API_KEY": "${SENDGRID_API_KEY}"
      }
    }
  }
}
